<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>

<GridPane hgap="10" prefHeight="700.0" prefWidth="800.0" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.Controller">
   <children>
      <Label text="Задать верхнюю границу M">
         <font>
            <Font size="14.0" />
         </font></Label>
      <ChoiceBox fx:id="choiceBox" prefHeight="40.0" prefWidth="120.0" GridPane.columnIndex="1" />
      <Button fx:id="solveButton" mnemonicParsing="false" prefHeight="40.0" prefWidth="120.0" text="Рассчитать" textAlignment="CENTER" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <Button fx:id="directoryButton" mnemonicParsing="false" prefHeight="40.0" prefWidth="120.0" text="Выбрать файл" textAlignment="CENTER" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Label text="Выбрать файл с исходными данными" GridPane.rowIndex="1">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label text="Решить задачу с заданными исходными данными" GridPane.rowIndex="2">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextArea fx:id="outputTextArea" prefHeight="530.0" prefWidth="650.0" GridPane.rowIndex="3">
         <font>
            <Font size="14.0" />
         </font>
      </TextArea>
      <GridPane prefHeight="438.0" prefWidth="120.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints maxHeight="40.0" minHeight="40.0" prefHeight="40.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="90.0" minHeight="90.0" prefHeight="90.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="60.0" minHeight="60.0" prefHeight="60.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="60.0" minHeight="60.0" prefHeight="60.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="60.0" minHeight="60.0" prefHeight="60.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label alignment="CENTER" prefHeight="20.0" prefWidth="120.0" text="Справка">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Button fx:id="referenceButton1" mnemonicParsing="false" prefHeight="70.0" prefWidth="120.0" text="Задача с фиксированными доплатами" textAlignment="CENTER" wrapText="true" GridPane.rowIndex="1" />
            <Button fx:id="referenceButton2" mnemonicParsing="false" prefHeight="40.0" prefWidth="120.0" text="Метод Балинского" textAlignment="CENTER" GridPane.rowIndex="2" />
            <Button fx:id="referenceButton3" mnemonicParsing="false" prefHeight="40.0" prefWidth="120.0" text="Симплекс-метод" textAlignment="CENTER" GridPane.rowIndex="3" />
            <Button fx:id="referenceButton4" mnemonicParsing="false" prefHeight="40.0" prefWidth="120.0" text="Входные данные" GridPane.rowIndex="4" />
         </children>
      </GridPane>
   </children>
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints minWidth="10.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</GridPane>
